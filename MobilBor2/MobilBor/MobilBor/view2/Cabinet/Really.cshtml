@model MobilBor.Models.Users
@{
    ViewBag.Title = "Учириш";
    Layout = "~/Views/Shared/View.cshtml";
}
@if (Session["User"].ToString() == "1")
{
<h2>Сиз хакикатдан хам бу ходимни админ сафидан чикармокчимисиз ?</h2>
    using (Html.BeginForm("Really", "Cabinet", FormMethod.Post))
    {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })


<div class="panel panel-primary no-border" style="border:0px;">
    <div class="panel-heading h1" style="margin-bottom:0px;"><p class="h3">Маълумотнома</p></div>
    <div class="panel-body" style="padding-top:0px; padding-bottom:0em; padding-left:0px; padding-right:0px;">
        <table id="datatable" class="table table-bordered table-striped" style="padding-bottom:0em; margin-bottom:0px;">
            <tr>
                <th>
                    @Html.DisplayName("ID")
                </th>
                <th>
                    @Html.DisplayName("Ф.И.О.")
                </th>
                <th>
                    @Html.DisplayName("Тел. номер")
                </th>
                <th>
                    @Html.DisplayName("Логин")
                </th>
                <th>
                    @Html.DisplayName("Пароль")
                <th>
                    @Html.DisplayName(" ")
                </th>
            </tr>

           
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => Model.ID)
                    </td>

                    <td>
                        @Html.ActionLink(Model.FullName, "Details", new { id = Model.ID })
                    </td>
                    <td>
                        @Model.Phone
                    </td>
                    <td>
                        @Model.Login
                    </td>
                    <td>
                        @Model.Password
                    </td>
                    @if (Model.Type == 1)
                    {
                    <td>
                        <button type="submit" class="fa fa-lock"></button>
                    </td>
                    }
                    else
                    {
                        <td>
                            <button type="submit" class="fa fa-unlock"></button>
                        </td>
                    }
                    
                </tr>
          

        </table>
    </div>
</div>

    @Html.HiddenFor(model => Model.ID)
    }
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/custom-validator")
}

