        var options = {
            legend: false,
            responsive: false
        };

        new Chart(document.getElementById("canvas1"), {
            type: 'doughnut',
            tooltipFillColor: "rgba(51, 51, 51, 0.55)",
            data: {
                labels: [
                  "Новые",
                  "Открытые",
                  "Закрытые",
                  "Перенаправленные",
                  "Треб. подтв."
                ],
                datasets: [{
