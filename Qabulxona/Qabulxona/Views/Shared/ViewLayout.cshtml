<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    <link type="text/css" href="~/Content/Style.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/StyleDate.css">
    <link href="~/Content/Ec/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Ec/bootstrap/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/Ec/bootstrap/css/ionicons.css" rel="stylesheet" />
    <link href="~/Content/Ec/dist/css/AdminLTE.css" rel="stylesheet" />
    <link href="~/Content/Ec/dist/css/skins/_all-skins.min.css" rel="stylesheet" />
    <link href="~/Content/Ec/bootstrap/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/Ec/plugins/iCheck/all.css" rel="stylesheet" />
    <link href="~/Content/Ec/plugins/jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" />
    <link href="~/Content/Ec/plugins/datepicker/datepicker3.css" rel="stylesheet" />
    <link href="~/Content/Ec/plugins/daterangepicker/daterangepicker-bs3.css" rel="stylesheet" />
    <link href="~/Content/Ec/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    @Scripts.Render("~/Scripts/jquery-1.10.2.js")
    @Scripts.Render("~/Scripts/jquery.unobtrusive-ajax.js")
    @Scripts.Render("~/bundles/modernizr")
    <script src="@Url.Content("~/Scripts/jquery-ui-1.11.4.min.js")" type="text/javascript"></script>
    <script src="~/Scripts/jquery.signalR-2.2.1.min.js"></script>
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    @RenderSection("documentLoad", required: false)
</head>
<body class="skin-blue fixed sidebar-mini">
    <div class="wrapper row-offcanvas row-offcanvas-left">
        <header class="main-header">
            <a href="/Control/IndexNew" class="logo">
                <span class="logo-lg">vazir.edu.uz</span>
            </a>
            <nav class="navbar navbar-static-top" role="navigation">
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li>
                            <a href="/Admin/Exit/">
                                <span>Chiqish</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <aside class="main-sidebar fixed">
            <section class="sidebar" style="text-align:center">
                @*@Html.Action("UserInfo", "Nav")*@
                <img src="~/Models/Images/gerb.png" style="width:114px; height:124px; padding-top:1em;" />
                <hr />
                <ul class="sidebar-menu">
                    <li>
                        <a href="/Control/IndexNew/">
                            <span>Bosh sahifa</span>
                        </a>
                    </li>
                    <li>
                        <a href="/Control/IndexNew/">
                            <span>Yangi murojaatlar</span>
                        </a>
                    </li>
                    <li>
                        <a href="/Control/IndexOld/">
                            <span>Ochiq murojaatlar</span>
                        </a>
                    </li>
                    <li>
                        <a href="/Control/IndexClosed/">
                            <span>Yopiq murojaatlar</span>
                        </a>
                    </li>
                </ul>
            </section>
        </aside>
        <div class="content-wrapper">
            <section class="content">
                @RenderBody()
            </section>
        </div>
    </div>

    <script src="@Url.Content("~/Scripts/DateTimeJs.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/plugins/sparkline/jquery.sparkline.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/plugins/jvectormap/jquery-jvectormap-world-mill-en.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/plugins/knob/jquery.knob.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/plugins/ute/moment.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/plugins/daterangepicker/daterangepicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/plugins/datepicker/bootstrap-datepicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/plugins/slimScroll/jquery.slimscroll.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/plugins/fastclick/fastclick.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Ec/dist/js/app.min.js")" type="text/javascript"></script>
    <script src="~/Content/Ec/plugins/iCheck/icheck.min.js"></script>
</body>
</html>
